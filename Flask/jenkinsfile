pipeline {
   agent any
   
   stages {
       stage('Login, Build, Push') {
           steps {
             dir("${env.WORKSPACE}/Flask"){
                sh "pwd"
                script {
                withDockerRegistry(credentialsId: 'DockerTeam1') {
                docker.build('anne07/team1-project').push('latest')
                     }
                    }
                }
           }
       }
   }
}   